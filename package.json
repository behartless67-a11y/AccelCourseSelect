{
  "name": "accel-course-selection",
  "version": "1.0.0",
  "description": "Real-time course selection system for Frank Batten School accelerated program",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "db:migrate": "node server/database/migrate.js",
    "algorithm": "python server/algorithm/optimize.py"
  },
  "keywords": ["course-selection", "real-time", "websocket", "optimization"],
  "author": "Frank Batten School",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "csv-parse": "^5.5.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
